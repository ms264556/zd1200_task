alias ampdu='iwpriv wlan0 ampdu'
alias gampdu='iwpriv wlan0 get_ampdu'
alias ant='sysctl -w dev.rks.ant=\'
alias gant='sysctl dev.rks.ant'
alias bant='sysctl -w dev.rks.bant=\'
alias gbant='sysctl dev.rks.bant'
alias chan='iwconfig wlan0 channel'
alias gchan='iwlist wlan0 channel'
alias cw='iwpriv wlan0 cwmmode'
alias gcw='iwpriv wlan0 get_cwmmode'
alias d='sysctl -w dev.rks.debug=\'
alias h='sysctl -w dev.wifi0.hackedreset=\'
alias h1='sysctl -w dev.wifi1.hackedreset=\'
alias lcm='iwpriv wlan0 tx_cm_legacy'
alias glcm='iwpriv wlan0 get_tx_cm_legacy'
alias locdepth='sysctl -w dev.rks.learn_locprobedepth=\'
alias glocdepth='sysctl dev.rks.learn_locprobedepth'
alias locint='sysctl -w dev.rks.initial_learn_delay=\'
alias glocint='sysctl dev.rks.initial_learn_delay'
alias locrate='sysctl -w dev.rks.learn_locproberate=\'
alias glocrate='sysctl dev.rks.learn_locproberate'
alias mqstats='rkscli -c "get mqstats svcp all"'
alias mqstatsclr='rkscli -c "set mqstats svcp clear all"'
alias n='sysctl -w dev.wifi0.nf=\'
alias n1='sysctl -w dev.wifi1.nf=\'
alias gn='athstats | grep NF'
alias gn1='athstats -i wifi1 | grep NF'
alias noreset='sysctl -w dev.ath.noreset=\'
alias gnoreset='sysctl dev.ath.noreset'
alias ns=nodestats
alias r='rkscli -c'
alias rate='sysctl -w dev.rks.forcedrate=\'
alias grate='sysctl dev.rks.forcedrate'
alias rcm='iwpriv wlan0 rx_chainmask'
alias grcm='iwpriv wlan0 get_rx_chainmask'
alias retry='sysctl -w dev.rks.forcedretry=\'
alias gretry='sysctl dev.rks.forcedretry'
alias rxant='sysctl -w dev.wifi0.rxant=\'
alias grxant='sysctl dev.wifi0.rxant'
alias rxant1='sysctl -w dev.wifi1.rxant=\'
alias grxant1='sysctl dev.wifi1.rxant'
alias rxdis='reg 0x8048=0x40000020'
alias rxen='reg 0x8048=0x40000000'
alias rxinact='sysctl -w dev.wifi0.rxinactthr_ms=\'
alias grrxinact='sysctl dev.wifi0.rxinactthr_ms'
alias rxinact1='sysctl -w dev.wifi1.rxinactthr_ms=\'
alias grrxinact1='sysctl dev.wifi1.rxinactthr_ms'
alias setsock='sysctl -w net.core.rmem_default=2000000; sysctl -w net.core.rmem_max=2000000'
alias tcm='iwpriv wlan0 tx_chainmask'
alias gtcm='iwpriv wlan0 get_tx_chainmask'
alias wl='wlanconfig wlan0 list'
alias wl1='wlanconfig wlan1 list'
alias ifupdn='ifconfig wlan0 down; ifconfig wlan0 up'
alias ppm='sysctl -w dev.ath.forcePpmEnable=\'
alias gppm='reg 0x9810'
alias ppmreset='reg 0x9810=0x7d14e000'
alias ppmreg='reg 0x9810=\'
alias aths0='athstats a'
alias aths1='athstats -i wifi1 a'
alias adbg0='athdebug -i wifi0'
