alias glocrate='sysctl dev.rks.learn_locproberate'
alias mqstats='rkscli -c "get mqstats svcp all"'
alias mqstatsclr='rkscli -c "set mqstats svcp clear all"'
alias n='sysctl -w dev.wifi0.nf=\'
alias gn='athstats | grep NF'
alias noreset='sysctl -w dev.ath.noreset=\'
alias gnoreset='sysctl dev.ath.noreset'
alias ns=nodestats
