#This is config file for USB tool
_DEF_ACTION="k s b"

#CF Card Device Information
#_CFCARD="hda"
#_BOOTFS="hda1"
#_ROOTFS="hda2 hda3"
#_DATAFS="hda4"

#Image Information
_PARTITION="partition.${PRODUCTINFO}.img"
_BOOTFSIMG="bootfs.i386.ext2.${PRODUCTINFO}.img"
_BOOTMENU="lib/grub/i386-pc/menu.lst"
_ROOTFSIMG="rootfs.i386.ext2.${PRODUCTINFO}.img"
_DATAFSIMG="datafs.i386.tar.${PRODUCTINFO}.img"
_KERNELIMG="bzImage"
_RESTOREIMG="restoreinitramfs.gz"
_RESTOREVER="restoreinitramfs.ver"

#Default Settings
#default settings for network boot
_DEF_KERNEL="bzImage"
_DEF_INITRAMFS="initramfs.gz"
_DEF_NFS_ROOT="/nfsroot"

#extra kernel command
_DEF_TFTP_KERNEL_CMD="console=ttyS0,115200n8"
_DEF_NFS_KERNEL_CMD="console=ttyS0,115200n8"

#default host/server network settings
_DEF_SERVER_IP="192.168.0.3"
_DEF_HOST_IP="192.168.0.2"
_DEF_NET_MASK="255.255.255.0"

#name of port1 and port2
_DEF_PORT1="eth0"
_DEF_PORT2="eth1"

#default connected port
_DEF_PORT="eth1"

#file name for downloading from server
_DEF_TEMP_FILE="/tmp/upg.img"

#list number of sector that going to be clean
_DEF_SECTOR_CLEAN_LIST="0 63 7688 20160 53816 509040 3920881 3921180 3927001"

#board data source [configfile/server]
_DEF_BOARD_DATA_SRC="configfile"

#default board data
_DEF_BOARD_DATA_FIELD="{board-name} {model-name} {serial-#} {OUI} {mac1} {mac2} customer"
_DEF_BOARD_DATA_T5520UR="bassethund ZD5010 00000000 00:1f:41 07:18:00 07:19:00 ruckus"
_DEF_BOARD_DATA_NAR5520="dachshund ZD3050 00000000 00:1f:41 00:07:18 00:07:19 ruckus"
_DEF_BOARD_DATA_COB7402="inu ZD1200 123456789012 6c:aa:b3 00:00:a0 00:00:a1 ruckus"

#board data file name fro downloading from server
_DEF_BOARD_DATA_FILE="zd_boarddata.txt"
